<html>
    <head>
        <title>Messages</title>
    </head>
    <body>
        <h1>Messages</h1>
        <button><a href="/new">Add new message</a></button>
        <ul>
            <% for (let i = 0; i < messages.length; i++) { %>
                <p><%= messages[i].text %></p>
                <p><%= messages[i].user %></p>
                <p><%= messages[i].added %></p>
            <% } %>
        </ul>
    </body>
</html>